    <description>Boilerplate for GoldysGym Case Study</description>

    <properties>
        <java.version>11</java.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
        </dependency>
    </dependencies>

    <modules>
        <module>ConfigService</module>
        <module>EurekaService</module>
        <module>ApiGatewayService</module>
        <module>UserService</module>
        <module>GymService</module>
        <module>EnquiryService</module>
        <module>TicketService</module>
    </modules>

</project>
